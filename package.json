{
  "name": "portfolio",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=4.0.0"
  },
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "node scripts/check-node-version.js && vitest run",
    "test:watch": "node scripts/check-node-version.js && vitest",
    "test:ui": "node scripts/check-node-version.js && vitest --ui",
    "test:coverage": "node scripts/check-node-version.js && vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "yarn test && yarn test:e2e",
    "analyze": "source-map-explorer 'build/assets/*.js'",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint": "node scripts/lint-with-progress.mjs && yarn format:check",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:css:fix && yarn format",
    "lint:eslint": "eslint src/**/*.{js,jsx,ts,tsx} tests/**/*.{js,jsx,ts,tsx}",
    "lint:eslint:fix": "eslint src/**/*.{js,jsx,ts,tsx} tests/**/*.{js,jsx,ts,tsx} --fix",
    "lint:types": "tsc --noEmit",
    "lint:css": "stylelint \"src/**/*.css\" \"tests/**/*.css\"",
    "lint:css:fix": "stylelint \"src/**/*.css\" \"tests/**/*.css\" --fix",
    "lint:commits": "commitlint --edit",
    "browserslist:update": "yarn update-browserslist-db@latest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,scss,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "bash -c 'tsc --noEmit'"
    ]
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "classnames": "^2.5.1",
    "react": "^19.2.3",
    "react-animate-on-scroll": "^2.1.9",
    "react-dom": "^19.2.3",
    "react-inlinesvg": "^4.2.0",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@playwright/test": "^1.48.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "prettier-plugin-css-order": "^2.1.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "semantic-release": "^24.2.0",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^16.0.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.16"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
